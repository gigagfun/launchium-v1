# Pinata IPFS Configuration
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here

# Solana Wallet Configuration
SOLANA_PRIVATE_KEY=your_solana_private_key_base58_here

# CORS Configuration (comma-separated URLs)
ALLOWED_ORIGINS=https://your-frontend.vercel.app,https://your-custom-domain.com

# Railway Backend URL (for frontend reference)
RAILWAY_BACKEND_URL=https://your-backend.railway.app

# Node Environment
NODE_ENV=production

# Optional: Custom port (Railway will provide this automatically)
# PORT=3001